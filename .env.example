# Server Configuration
PORT=5001
NODE_ENV=development

# Database Configuration
DATABASE_URL="postgresql://postgres:130230@localhost:5432/postgres"

# JWT Configuration
JWT_ACCESS_TOKEN_SECRET=your-super-secret-access-token-key-here
JWT_REFRESH_TOKEN_SECRET=your-super-secret-refresh-token-key-here
JWT_ACCESS_TOKEN_EXPIRATION=10h
JWT_REFRESH_TOKEN_EXPIRATION=7d

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_CALLBACK_URL=http://localhost:5001/auth/google/callback

# Email Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USER=your-email@gmail.com
MAIL_PASS=your-app-password

# MinIO/S3 Configuration
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
